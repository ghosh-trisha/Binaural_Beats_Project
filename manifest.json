{
    "manifest_version" : 3,
    "name" : "Binaural Beats",
    "version" : "1.0.0",
    "permissions" : ["tabs"],
    "action" : {
        "default_popup" : "popup.html",
        "default_icon" : "logo.jpg"
    },
    "background" : {
        "service_worker" : "background.js"
    },
    "content_scripts" : [
        {
            "js" : ["content.js"],
            "matches" : ["<all_urls>"]
        }
    ],
    "icons" : {
        "128" : "logo.jpg"
    }

}